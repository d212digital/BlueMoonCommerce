@if (AppDataHelper.IsRTL)
{
    <link href="https://fonts.googleapis.com/css2?family=Tajawal:wght@400;700&display=swap" rel="stylesheet">

    @Styles.Render("~/bundles/dashboard/content/rtl/css")
    @Scripts.Render("~/bundles/dashboard/content/scripts")

}
else
{
    <link href="https://fonts.googleapis.com/css2?family=Karla:wght@400;700;800&display=swap" rel="stylesheet">

    @Styles.Render("~/bundles/dashboard/content/css")
    @Scripts.Render("~/bundles/dashboard/content/scripts")

    <!-- Page level plugins -->
    <script src="~/Content/templates/sbadmin2/vendor/chart.js/Chart.min.js"></script>
    <script src="~/Content/templates/sbadmin2/js/sb-admin-2.min.js"></script>

    <!-- Bootstrap core JavaScript-->

    <script src="~/Content/templates/sbadmin2/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Content/templates/sbadmin2/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/templates/sbadmin2/js/demo/chart-pie-demo.js"></script>
    <script src="~/Content/templates/sbadmin2/js/demo/chart-area-demo.js"></script>


}